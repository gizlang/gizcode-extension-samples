{
  "activationEvents": [ "onLanguage:json", "onLanguage:jsonc" ],
  "categories": [ "Other" ],
  "contributes": {
    "commands": [ {
      "command": "nodeDependencies.refreshEntry",
      "icon": {
        "dark": "resources/dark/refresh.svg",
        "light": "resources/light/refresh.svg"
      },
      "title": "Refresh"
    }, {
      "command": "nodeDependencies.addEntry",
      "title": "Add"
    }, {
      "command": "nodeDependencies.editEntry",
      "icon": {
        "dark": "resources/dark/edit.svg",
        "light": "resources/light/edit.svg"
      },
      "title": "Edit"
    }, {
      "command": "nodeDependencies.deleteEntry",
      "title": "Delete"
    }, {
      "command": "ftpExplorer.refresh",
      "icon": {
        "dark": "resources/dark/refresh.svg",
        "light": "resources/light/refresh.svg"
      },
      "title": "Refresh"
    }, {
      "command": "ftpExplorer.openFtpResource",
      "title": "Open FTP Resource"
    }, {
      "command": "ftpExplorer.revealResource",
      "title": "Reveal in FTP View"
    }, {
      "command": "jsonOutline.refresh",
      "icon": {
        "dark": "resources/dark/refresh.svg",
        "light": "resources/light/refresh.svg"
      },
      "title": "Refresh"
    }, {
      "command": "jsonOutline.refreshNode",
      "icon": {
        "dark": "resources/dark/refresh.svg",
        "light": "resources/light/refresh.svg"
      },
      "title": "Refresh"
    }, {
      "command": "jsonOutline.renameNode",
      "title": "Rename"
    }, {
      "command": "fileExplorer.refreshFile",
      "icon": {
        "dark": "resources/dark/refresh.svg",
        "light": "resources/light/refresh.svg"
      },
      "title": "Refresh"
    }, {
      "command": "fileExplorer.openFile",
      "title": "Open File"
    }, {
      "command": "testView.reveal",
      "title": "Test View: Reveal"
    }, {
      "command": "testView.changeTitle",
      "title": "Test View: Change Title"
    } ],
    "configuration": [ {
      "properties": {
        "jsonOutline.autorefresh": {
          "default": false,
          "type": "boolean"
        }
      },
      "title": "JSON Outline"
    } ],
    "menus": {
      "commandPalette": [ {
        "command": "ftpExplorer.revealResource"
      } ],
      "view/item/context": [ {
        "command": "nodeDependencies.editEntry",
        "group": "inline",
        "when": "view == nodeDependencies && viewItem == dependency"
      }, {
        "command": "nodeDependencies.deleteEntry",
        "when": "view == nodeDependencies && viewItem == dependency"
      }, {
        "command": "jsonOutline.renameNode",
        "when": "view == jsonOutline"
      }, {
        "command": "jsonOutline.refreshNode",
        "group": "inline",
        "when": "view == jsonOutline"
      }, {
        "command": "fileExplorer.refreshFile",
        "group": "inline",
        "when": "view == fileExplorer && viewItem == file"
      } ],
      "view/title": [ {
        "command": "nodeDependencies.refreshEntry",
        "group": "navigation",
        "when": "view == nodeDependencies"
      }, {
        "command": "jsonOutline.renameNode",
        "when": "view == jsonOutline"
      }, {
        "command": "nodeDependencies.addEntry",
        "when": "view == nodeDependencies"
      }, {
        "command": "jsonOutline.refresh",
        "group": "navigation",
        "when": "view == jsonOutline"
      }, {
        "command": "ftpExplorer.refresh",
        "group": "navigation",
        "when": "view == ftpExplorer"
      } ]
    },
    "views": {
      "explorer": [ {
        "id": "jsonOutline",
        "name": "Json Outline",
        "when": "jsonOutlineEnabled"
      }, {
        "id": "ftpExplorer",
        "name": "FTP Explorer"
      }, {
        "id": "fileExplorer",
        "name": "File Explorer"
      }, {
        "id": "testView",
        "name": "Test View"
      }, {
        "id": "testViewDragAndDrop",
        "name": "Test View Drag and Drop"
      } ],
      "package-explorer": [ {
        "contextualTitle": "Package Explorer",
        "icon": "media/dep.svg",
        "id": "nodeDependencies",
        "name": "Node Dependencies"
      } ]
    },
    "viewsContainers": {
      "activitybar": [ {
        "icon": "media/dep.svg",
        "id": "package-explorer",
        "title": "Package Explorer"
      } ]
    }
  },
  "dependencies": {
    "ftp": "^0.3.10",
    "jsonc-parser": "^0.4.2",
    "minimist": "^1.2.6",
    "mkdirp": "^0.5.1",
    "rimraf": "^2.7.1"
  },
  "description": "Samples for VSCode's view API",
  "devDependencies": {
    "@types/ftp": "^0.3.33",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "^16.11.7",
    "@types/rimraf": "^2.0.2",
    "@types/vscode": "^1.73.0",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.26.0",
    "typescript": "^5.0.2"
  },
  "displayName": "Custom view Samples",
  "engines": {
    "vscode": "^1.74.0"
  },
  "license": "MIT",
  "main": "./out/extension.js",
  "name": "custom-view-samples",
  "private": true,
  "publisher": "vscode-samples",
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/vscode-extension-samples"
  },
  "scripts": {
    "compile": "tsc -p ./",
    "lint": "eslint \"src/**/*.ts\"",
    "vscode:prepublish": "npm run compile",
    "watch": "tsc -watch -p ./"
  },
  "version": "0.0.1"
}